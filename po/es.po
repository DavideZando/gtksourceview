#: gtksourceview/gtksourcebuffer.c:279
msgid "Escape Character"
msgstr "Carácter de escape"

#: gtksourceview/gtksourcebuffer.c:280
msgid "Escaping character for syntax patterns"
msgstr "Indica el carácter de escape para los patrones de sintaxis."

#: gtksourceview/gtksourcebuffer.c:288
msgid "Check Brackets"
msgstr "Comprobar corchetes"

#: gtksourceview/gtksourcebuffer.c:289
msgid "Whether to check and highlight matching brackets"
msgstr "Indica si se comprueban y resaltan los corchetes coincidentes"

#: gtksourceview/gtksourcebuffer.c:297 gtksourceview/gtksourceprintjob.c:303
msgid "Highlight"
msgstr "Resaltar"

#: gtksourceview/gtksourcebuffer.c:298
msgid "Whether to highlight syntax in the buffer"
msgstr "Indica si se resalta la sintaxis en el búfer"

#: gtksourceview/gtksourcebuffer.c:306
msgid "Maximum Undo Levels"
msgstr "Nivel máximo de deshacer"

#: gtksourceview/gtksourcebuffer.c:307
msgid "Number of undo levels for the buffer"
msgstr "Número de niveles de deshacer para el archivo."

#: gtksourceview/gtksourcebuffer.c:317
msgid "Language"
msgstr "Lenguaje"

#: gtksourceview/gtksourcebuffer.c:318
msgid "Language object to get highlighting patterns from"
msgstr "El objeto de lenguaje del que se obtienen los patrones de resaltado"

#: gtksourceview/gtksourcelanguagesmanager.c:119
msgid "Language specification directories"
msgstr "Directorios de especificaciones de lenguajes"

#: gtksourceview/gtksourcelanguagesmanager.c:120
msgid ""
"List of directories where the language specification files (.lang) are "
"located"
msgstr ""
"Lista de directorios en los que se hallan los archivo de especificación de "
"lenguajes (.lang)."

#: gtksourceview/gtksourceprintjob.c:272
msgid "Configuration"
msgstr "Configuración"

#: gtksourceview/gtksourceprintjob.c:273
msgid "Configuration options for the print job"
msgstr "Opciones de configuración para el trabajo de impresión"

#: gtksourceview/gtksourceprintjob.c:280
msgid "Source Buffer"
msgstr "Búfer del fuente"

#: gtksourceview/gtksourceprintjob.c:281
msgid "GtkSourceBuffer object to print"
msgstr "Objeto GtkSourceBuffer a imprimir"

#: gtksourceview/gtksourceprintjob.c:287 gtksourceview/gtksourceview.c:223
#: gtksourceview/gtksourceview.c:224
msgid "Tabs Width"
msgstr "Ancho de tabulación"

#: gtksourceview/gtksourceprintjob.c:288
msgid "Width in equivalent space characters of tabs"
msgstr "Anchura en espacios equivalentes para los tabuladores"

#: gtksourceview/gtksourceprintjob.c:295
msgid "Wrap Mode"
msgstr "Modo de ajuste"

#: gtksourceview/gtksourceprintjob.c:296
msgid "Word wrapping mode"
msgstr "Modo de ajuste de palabras"

#: gtksourceview/gtksourceprintjob.c:304
msgid "Whether to print the document with highlighted syntax"
msgstr "Indica si se imprime el documento con resaltado de sintaxis"

#: gtksourceview/gtksourceprintjob.c:312
msgid "Font"
msgstr "Tipografía"

#: gtksourceview/gtksourceprintjob.c:313
msgid "Font name to use for the document text"
msgstr "Nombre de la tipografía para el texto del documento"

#: gtksourceview/gtksourceprintjob.c:320
msgid "Numbers Font"
msgstr "Tipografía para los números"

#: gtksourceview/gtksourceprintjob.c:321
msgid "Font name to use for the line numbers"
msgstr "Nombre de la tipografía a usar en los números de línea"

#: gtksourceview/gtksourceprintjob.c:328
msgid "Print Line Numbers"
msgstr "Mostrar números de líneas"

#: gtksourceview/gtksourceprintjob.c:329
msgid "Interval of printed line numbers (0 means no numbers)"
msgstr "Intervalo de impresión de números de líneas (0 significa sin números)"

#: gtksourceview/gtksourceprintjob.c:336
msgid "Print Header"
msgstr "Imprimir cabecera"

#: gtksourceview/gtksourceprintjob.c:337
msgid "Whether to print a header in each page"
msgstr "Indica si se imprimirá una cabecera en cada página"

#: gtksourceview/gtksourceprintjob.c:344
msgid "Print Footer"
msgstr "Imprimir pie de página"

#: gtksourceview/gtksourceprintjob.c:345
msgid "Whether to print a footer in each page"
msgstr "Indica si se imprimirá un pie de página en cada página"

#: gtksourceview/gtksourceprintjob.c:352
msgid "Header and Footer Font"
msgstr "Tipografía de cabecera y pie de página"

#: gtksourceview/gtksourceprintjob.c:353
msgid "Font name to use for the header and footer"
msgstr "Nombre de la tipografía a usar para la cabecera y el pie de página"

#: gtksourceview/gtksourcestylescheme.c:230
