#: gtksourceview/gtksourceview.c:207
msgid "Show Line Numbers"
msgstr "Mostrar número de línea"

#: gtksourceview/gtksourceview.c:208
msgid "Whether to display line numbers"
msgstr "Indica si se mostrarán los números de las líneas."

#: gtksourceview/gtksourceview.c:215
msgid "Show Line Markers"
msgstr "Mostrar marcadores de líneas"

#: gtksourceview/gtksourceview.c:216
msgid "Whether to display line marker pixbufs"
msgstr "Indica si se mostrarán los marcadores de líneas."

#: gtksourceview/gtksourceview.c:223
#: gtksourceview/gtksourceview.c:224
msgid "Tabs Width"
msgstr "Ancho de tabulación"

#: gtksourceview/gtksourceview.c:233
msgid "Auto Indentation"
msgstr "Autoindentación"

#: gtksourceview/gtksourceview.c:234
msgid "Whether to enable auto indentation"
msgstr "Indica si se habilita la indentación automática."

#: gtksourceview/gtksourceview.c:240
msgid "Insert Spaces Instead of Tabs"
msgstr "Insertar espacios en vez de tabuladores"

#: gtksourceview/gtksourceview.c:241
msgid "Whether to insert spaces instead of tabs"
msgstr "Indica si se insertarán espacios en vez de tabuladores."

#: gtksourceview/gtksourceview.c:248
msgid "Show Right Margin"
msgstr "Mostrar margen derecho"

#: gtksourceview/gtksourceview.c:249
msgid "Whether to display the right margin"
msgstr "Indica si se mostrará el margen derecho."

#: gtksourceview/gtksourceview.c:256
msgid "Margin position"
msgstr "Posición del margen"

#: gtksourceview/gtksourceview.c:257
msgid "Position of the right margin"
msgstr "Posición del margen derecho"

#: gtksourceview/gtksourceview.c:266
msgid "Use smart home/end"
msgstr "Utilizar inicio/fin inteligente"

#: gtksourceview/gtksourceview.c:267
msgid "HOME and END keys move to first/last characters on line first before going to the start/end of the line"
msgstr "Las teclas «Inicio» y «Fin» situarán el cursor en el primer/último carácter de la línea antes de ir al inicio/fin de la línea."

#: gtksourceview/gtksourcebuffer.c:277
msgid "Escape Character"
msgstr "Carácter de escape"

#: gtksourceview/gtksourcebuffer.c:278
msgid "Escaping character for syntax delimiters"
msgstr "Indica el carácter de espace para los delimitadores de sintaxis."

#: gtksourceview/gtksourcebuffer.c:286
msgid "Check Brackets"
msgstr "Comprobar corchetes"

#: gtksourceview/gtksourcebuffer.c:287
msgid "Whether to check and highlight matching brackets"
msgstr "Indica si se comprueban y resaltan los corchetes correspondientes."

#: gtksourceview/gtksourcebuffer.c:295
msgid "Highlight"
msgstr "Resaltar"

#: gtksourceview/gtksourcebuffer.c:296
msgid "Whether to syntax highlight the buffer"
msgstr "Indica si se resalta la sintaxis del archivo."

#: gtksourceview/gtksourcebuffer.c:304
msgid "Maximum Undo Levels"
msgstr "Nivel máximo de deshacer"

#: gtksourceview/gtksourcebuffer.c:305
msgid "Number of undo levels for the buffer"
msgstr "Número de niveles de deshacer para el archivo."

#: gtksourceview/gtksourcebuffer.c:315
msgid "Language"
msgstr "Lenguaje"

#: gtksourceview/gtksourcebuffer.c:316
msgid "Language object to get highlighting rules from"
msgstr "El objeto de lenguaje del que se obtienen las reglas de resaltado."

#: gtksourceview/gtksourcelanguagesmanager.c:119
msgid "Language spefications directories"
msgstr "Directorios de especificaciones de lenguajes"

#: gtksourceview/gtksourcelanguagesmanager.c:120
msgid "List of directories where the language spefication files (.lang) are located"
msgstr "Lista de directorios en los que se hallan los archivo de especificación de lenguajes (.lang)."

